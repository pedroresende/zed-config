{
  "preview_tabs": {
    "enabled": false
  },
  "file_types": {
    "erb": ["*.erb"]
  },
  "use_on_type_format": true,
  "show_edit_predictions": true,
  "ensure_final_newline_on_save": true,
  "wrap_guides": [80, 120],
  "tab_size": 2,
  "autosave": "on_window_change",
  "base_keymap": "VSCode",
  "minimap": {
    "show": "auto"
  },
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "ollama",
      "model": "gpt-oss:20b-cloud"
    }
  },
  "icon_theme": "Catppuccin Macchiato",
  "features": {
    "edit_prediction_provider": "supermaven"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "show_whitespaces": "all",
  "ui_font_size": 16,
  "buffer_font_size": 13,
  "terminal": {
    "env": {
      "Q_NEW_SESSION": "1",
      "CW_NEW_SESSION": "1",
      "FIG_NEW_SESSION": "1"
    },
    "font_size": 13,
    "font_family": "Hack Nerd Font",
    "line_height": "standard"
  },
  "buffer_font_family": "Hack Nerd Font",
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Carbonfox - blurred"
  },
  "prettier": {
    "printWidth": [80, 120],
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "endOfLine": "lf"
  },
  "languages": {
    "Ruby": {
      "language_servers": ["ruby-lsp", "rubocop", "!solargraph"]
    }
  },
  "lsp": {
    "rubocop": {
      "initialization_options": {
        "safeAutocorrect": false
      }
    },
    "ruby-lsp": {
      "initialization_options": {
        "enabledFeatures": {
          "diagnostics": false
        }
      }
    }
  }
}
